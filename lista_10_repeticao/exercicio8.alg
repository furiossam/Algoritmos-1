algoritmo "semnome"
// Função :
// Autor :
// Data : 20/04/2017
// Seção de Declarações 
var
   biscoitos,total,cont:inteiro
inicio
// Seção de Comandos 
   biscoitos <- 1
   total <- 1
   cont <- 0
   enquanto (cont < 15) faca
        biscoitos <- biscoitos*3
        total <- total + biscoitos
        cont <- cont + 1
   fimenquanto
   escreval (total)

fimalgoritmo
