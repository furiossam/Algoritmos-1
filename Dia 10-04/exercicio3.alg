algoritmo "semnome"
// Função :
// Autor :
// Data : 10/04/2017
// Seção de Declarações 
var

    soma,contador:inteiro

inicio
// Seção de Comandos 
   soma <- 0
   contador <-1

   repita
         soma <- soma + contador
         contador <- contador + 1
   ate (contador > 100)
       escreva (soma)

fimalgoritmo
